{
  "name": "ai-image-studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://studiov.aemons.com",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "proxy": "node proxy-server.js",
    "asset-server": "node assetServer.js",
    "dev:all": "npm run proxy & npm run dev",
    "dev:all:host": "npm run proxy & npm run dev:host",
    "dev:vn": "npm run asset-server & npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod",
    "gallery:server": "node simple-server.cjs",
    "gallery:load": "node load-gallery-images.cjs",
    "gallery:help": "bash .gallery-helper.sh",
    "auto:status": "node scripts/autoPublish.mjs --status",
    "auto:publish": "node scripts/autoPublish.mjs",
    "auto:publish:dry": "node scripts/autoPublish.mjs --dry-run",
    "auto:quick": "node scripts/autoPublish.mjs --quick",
    "auto:publish:5": "node scripts/autoPublish.mjs --max 5",
    "auto:carousel": "node scripts/autoPublish.mjs --carousel",
    "auto:carousel:dry": "node scripts/autoPublish.mjs --carousel --dry-run",
    "auto:carousel:preview": "node scripts/autoPublish.mjs --preview",
    "auto:carousel:publish": "node scripts/autoPublish.mjs --publish-pending",
    "carousel:publish": "node scripts/publishCarousel.mjs",
    "carousel:schedule": "node scripts/carouselScheduler.mjs",
    "carousel:prepare": "node scripts/prepareCarousels.mjs",
    "auto:preview": "node scripts/generatePreview.mjs",
    "auto:preview:10": "node scripts/generatePreview.mjs 10",
    "auto:drive:test": "node scripts/testDriveConnection.mjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/genai": "^1.28.0",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^9.6.3",
    "googleapis": "^169.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "replicate": "^1.4.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.27.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}

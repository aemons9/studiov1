{
  "name": "ai-image-studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://studiov.aemons.com",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "proxy": "node proxy-server.js",
    "asset-server": "node assetServer.js",
    "dev:all": "npm run proxy & npm run dev",
    "dev:all:host": "npm run proxy & npm run dev:host",
    "dev:vn": "npm run asset-server & npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod",
    "gallery:server": "node simple-server.cjs",
    "gallery:load": "node load-gallery-images.cjs",
    "gallery:help": "bash .gallery-helper.sh",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "insta:capture": "node scripts/capture-instagram-posts.mjs",
    "insta:publish": "node scripts/quick-publish-instagram.mjs",
    "insta:status": "node scripts/quick-publish-instagram.mjs --status",
    "insta:preview": "node scripts/quick-publish-instagram.mjs --preview",
    "insta:next": "node scripts/quick-publish-instagram.mjs --next",
    "insta:day": "node scripts/quick-publish-instagram.mjs --day",
    "insta:carousel": "node scripts/quick-publish-instagram.mjs --carousel",
    "insta:batch": "node scripts/quick-publish-instagram.mjs --batch",
    "insta:batch-publish": "node scripts/quick-publish-instagram.mjs --batch-publish",
    "insta:batch-next": "node scripts/quick-publish-instagram.mjs --batch-next",
    "insta:batch-all": "node scripts/quick-publish-instagram.mjs --batch-all"
  },
  "dependencies": {
    "@google/genai": "^1.28.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "google-auth-library": "^9.6.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.34.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
